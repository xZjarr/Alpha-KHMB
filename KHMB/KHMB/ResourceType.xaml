<Window x:Class="KHMB.ResourceType"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KHMB"
        mc:Ignorable="d"
        Title="ResourceType" Height="300" Width="300">
    <Grid>
        <Button x:Name="Btn_Save" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="207,239,0,0" Click="Btn_Save_Click"/>
        <TextBox x:Name="Bx_Nm" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="0,91,0,0" TextChanged="TextBox_TextChanged"/>
        <Label Content="Resource Name" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,60,0,0" Width="110"/>

    </Grid>
</Window>
