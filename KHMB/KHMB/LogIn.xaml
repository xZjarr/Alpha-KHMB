<Window x:Class="KHMB.LogIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KHMB"
        mc:Ignorable="d"
        Title="LogIn" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="Txt_UserName" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="60,71,0,0" TextChanged="UserName_TextChanged"/>
        <Label Content="Username" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="60,40,0,0"/>
        <Label Content="Password" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="60,110,0,0"/>
        <PasswordBox x:Name="Txt_PassWord" HorizontalAlignment="Left" VerticalAlignment="Top" Height="23" Width="120" Margin="60,141,0,0"/>
        <Button x:Name="LogIn1" Content="Log In" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="207,239,0,0" Click="LogIn1_Click" IsDefault="True"/>
        <Button x:Name="Btn_Return" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="Btn_Return_Click"/>

    </Grid>
</Window>
